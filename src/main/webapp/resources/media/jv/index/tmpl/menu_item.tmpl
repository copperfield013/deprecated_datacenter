{{each(i, l1) menus}}
	 <li>
		<a href="#" class="menu-dropdown">
		    <i class="menu-icon fa fa-bookmark"></i>
		    <span class="menu-text">${l1.title }</span>
		</a>
		<ul class="submenu">
			{{each(j, level2) l1.level2s}}
				<li>
					{{if level2.templateGroupId}}
						<a class="tab" href="jv/entity/curd/list/${level2.id }" 
				   			target="entity_list_${level2.id }" title="${level2.title }">
				   			<span class="menu-text">${level2.title }</span>
				   		</a>
				   	{{else level2.statViewId}}
				   		<a class="tab" href="jv/stat/view/index/${level2.id }" 
				   			target="stat_view_${level2.statViewId }" title="${level2.title }">
				   			<span class="menu-text">${level2.title }</span>
				   		</a>
					{{/if}}
				</li>
			{{/each}}
		</ul>
	</li>
{{/each}}